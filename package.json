  "scripts": {
    "start": "run-p watch:css react-scripts:start NODE_ENV=development",
    "dev": "run-p watch:css react-scripts:start",
    "build": "react-scripts build",
    "start-prod": "NODE_ENV=production node scripts/start.js",
    "start-exp": "NODE_ENV=exp node scripts/start.js",
    "build-dev": "NODE_OPTIONS=--max_old_space_size=4096 yarn build-dev-env",
    "build-prod": "NODE_OPTIONS=--max_old_space_size=4096 yarn build-prod-env",
    "build-exp": "NODE_OPTIONS=--max_old_space_size=4096 yarn build-exp-env",
    "build-dev-env": "rm -f .env && cp environments/.env.development .env && yarn build",
    "build-prod-env": "rm -f .env && cp environments/.env.production .env && yarn build",
    "build-exp-env": "rm -f .env && cp environments/.env.exp .env && yarn build",
    "dev-prod": "yarn dev-prod-env",
    "dev-exp": "yarn dev-exp-env",
    "dev-prod-env": "rm -f .env && cp environments/.env.production .env && yarn dev",
    "dev-exp-env": "rm -f .env && cp environments/.env.exp .env && yarn dev",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "cross-env TAILWIND_MODE=build NODE_ENV=production postcss src/assets/styles/app.css -o src/index.css",
    "watch:css": "cross-env TAILWIND_MODE=watch NODE_ENV=development postcss src/assets/styles/app.css -o src/index.css --watch",
    "react-scripts:start": "delay 5 && react-scripts start",
    "react-scripts:build": "react-scripts build",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "eslintConfig": {
    "extends": [
